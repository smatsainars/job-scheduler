{
    "name": "yourname/job-scheduler-api",
    "description": "Job Scheduler API with GraphQL",
    "type": "project",
    "require": {
        "php": ">=8.1",
        "doctrine/dbal": "3.7",
        "doctrine/orm": "2.17",
        "firebase/php-jwt": "6.9",
        "google/apiclient": "2.15",
        "monolog/monolog": "3.5",
        "nesbot/carbon": "^3.9",
        "nikic/fast-route": "1.3",
        "ramsey/uuid": "4.7",
        "respect/validation": "2.2",
        "symfony/cache": "^7.3",
        "vlucas/phpdotenv": "5.5",
        "webonyx/graphql-php": "15.0"
    },
    "require-dev": {
        "symfony/var-dumper": "^7.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "start": "php -S localhost:8000 -t public",
        "doctrine": "./bin/doctrine",
        "test": "echo 'Tests not configured yet'"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
